<h1>Log into your Profile</h1>
<%= form_tag sessions_path do %>

  <div>
    <%= label_tag "email", "Email" %><br>
    <%= text_field_tag "email" %>
  </div>

  <div>
    <%= label_tag "password", "Password" %><br>
    <%= password_field_tag "password" %>
  </div>

  <%= submit_tag "Log in" %>

  <% end %>
<%= link_to "profile", profile_path %>
